package com.OneToOne.OneToOneEj.Model;

@Entity
public class Book {
	
	@Id
	
	@GeneratedValue
	private long idBook;
	
	private String name;
	
	@OneToOne(cascade = CascadeType.ALL)
	
	
	
	
	
	public Book(){}
	
	public Book(String name) {
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
}
