package com.ejercicio6;

import java.util.ArrayList;

public class Jugador {
	private ArrayList<Auto> listadeAutos;
	private String nombre,salario;
	private Club club;
	
	public Jugador(String nombre,String salario,Club club){
		this.nombre = nombre;
		this.salario = salario;
		this.club = club;
		this.listadeAutos = new ArrayList<>();
		
		club.getListaJugadores().add(this);
	}

}
