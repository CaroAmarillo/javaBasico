package com.ejercicio14;

public class Lavadora extends Electrodomestico{

    private final static int cargaPorDefecto=5;

    private int carga;
  

    
    public Lavadora(){
        this(precioBasePorDefecto, pesoPorDefecto, consumoEnergeticoPorDefecto, colorPorDefecto, cargaPorDefecto);
    }
  

    public Lavadora(double precioBase, double peso, char consumoenergeticopordefecto, String colorpordefecto, int cargapordefecto2){
        this(precioBase, peso, consumoEnergeticoPorDefecto, colorPorDefecto, cargaPorDefecto);
    }
  
    }
    public int getCarga() {
        return carga;
    }
    public double precioFinal(){
        double plus=super.precioFinal();
        if (carga>30){
            plus+=50;
        }
  
        return plus;
    }
}