
public class Persona {
	
	private String nombre;
	private int edad;
	private String dni;
	private final String sexoMasculino = "M";
	private String sexoFemenino = "F";
	private int peso;
	private int altura;
	
	public Persona(){
		
	}
	
	public Persona(String nom, String sexo, String dni){
		
		nombre = nom;
		sexo = sexoMasculino;
		this.dni = dni;	
		
	}
	
	

}
